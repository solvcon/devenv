. ${DEVENVROOT}/scripts/func.d/bash_utils

if [ ! -d "${DEVENVFLAVORROOT}/$1" ]; then
  mkdir -p ${DEVENVFLAVORROOT}/$1
  display "create '${1}' dev environment."
elif [ -z $1 ]; then
  display -e "must need an argument!!"
else
  display -e "'$1' already exist!!"
fi

